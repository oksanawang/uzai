angeState: function changeState(idx) {\n      //功能：点底部按钮切换图片状态\n      //当前的图片是选中的，其他的是默认的？\n      //参数idx：表示下标\n      // console.log(idx);\n      // 1.创建循环遍历状态与当前元素下标相同\n      for (var i = 0; i < this.currentIndex.length; i++) {\n        // 2.判断如果参数与当前元素下标相同\n        if (i == idx) {\n          // 3.当前元素状态修改true\n          this.currentIndex[i].isSelect = true;\n        } else {\n          // 4.其他元素状态修改false\n          this.currentIndex[i].isSelect = false;\n        }\n      }\n    }\n  }\n};",{"version":3,"sources":["Home.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgEA;AACA,OAAA,QAAA,MAAA,uBAAA,C,CACA;;AACA,OAAA,WAAA,MAAA,0BAAA,C,CACA;;AACA,OAAA,UAAA,MAAA,yBAAA;AAEA,eAAA;AACA,MADA,kBACA;AACA,WAAA;AACA,cAAA,SADA;AAEA,oBAAA,CACA;AAAA,kBAAA;AAAA,OADA,EACA;AACA;AAAA,kBAAA;AAAA,OAFA,EAEA;AACA;AAAA,kBAAA;AAAA,OAHA,EAGA;AACA;AAAA,kBAAA;AAAA,OAJA;AAFA,KAAA;AASA,GAXA;AAYA;AACA,cAAA;AACA,gBAAA,QADA;AACA;AACA,mBAAA,WAFA;AAEA;AACA,kBAAA,UAHA,CAGA;;AAHA,GAbA;AAkBA,WAAA;AACA;AACA,YAFA,sBAEA;AAAA,cAAA,GAAA,CAAA,IAAA;AAAA,KAFA;AAEA;AACA,UAHA,oBAGA;AAAA,cAAA,GAAA,CAAA,IAAA;AAAA,KAHA;AAGA;AACA;AACA;AACA,eANA,uBAMA,GANA,EAMA;AACA;AACA;AACA;AACA;AACA;AACA,WAAA,IAAA,IAAA,CAAA,EAAA,IAAA,KAAA,YAAA,CAAA,MAAA,EAAA,GAAA,EAAA;AACA;AACA,YAAA,KAAA,GAAA,EAAA;AACA;AACA,eAAA,YAAA,CAAA,CAAA,EAAA,QAAA,GAAA,IAAA;AACA,SAHA,MAGA;AACA;AACA,eAAA,YAAA,CAAA,CAAA,EAAA,QAAA,GAAA,KAAA;AACA;AACA;AACA;AAtBA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"page-tabbar\">\r\n    <div class=\"page-wrap\">\r\n     <!--父组件Home.vue-->\r\n     <!--1:顶部标题子组件-->\r\n     <titlebar\r\n        leftTitle=\"微信(๑乛◡乛๑卡在了奇怪的地方\"\r\n        :rightFirstImg=\"require('../../assets/ic_search.png')\"\r\n        :rightSecondImg=\"require('../../assets/ic_add.png')\"\r\n        :search=\"mysearch\"\r\n        :add3=\"myadd3\"\r\n        ></titlebar> \r\n        <!-- 2：保留空div（标题）  48高度 -->\r\n        <div style=\"margin-top:48px;\"></div>\r\n        <!-- Home.vue 入口全局组件 -->\r\n        <!-- 3：切换面板列表 -->\r\n        <mt-tab-container class=\"page-tabbar-container\" v-model=\"active\">\r\n            <!-- 子面板 -->\r\n            <mt-tab-container-item id=\"message\">\r\n                <!-- (2)引入消息列表 -->\r\n                <messagelist></messagelist>\r\n            </mt-tab-container-item>\r\n        </mt-tab-container>\r\n        <!--4.tabbar列表   底部导航条 -->\r\n        <mt-tabbar fixed v-model=\"active\">\r\n            <mt-tab-item id=\"message\" @click.native=\"changeState(0)\">\r\n                <!-- <img style=\"width:25px;\" src=\"../../assets/ic_weixin_normal.png\" alt=\"\"> -->\r\n                <tabbaricon\r\n                :focused=\"currentIndex[0].isSelect\"\r\n                :selectImage=\"require('../../assets/ic_weixin_selected.png')\"\r\n                :normalImage=\"require('../../assets/ic_weixin_normal.png')\"\r\n                ></tabbaricon>微信\r\n            </mt-tab-item>\r\n            <mt-tab-item id=\"contact\" @click.native=\"changeState(1)\">\r\n                <!-- <img style=\"width:25px;\" src=\"../../assets/ic_contacts_normal.png\" alt=\"\"> -->\r\n                <tabbaricon\r\n                :focused=\"currentIndex[1].isSelect\"\r\n                :selectImage=\"require('../../assets/ic_contacts_selected.png')\"\r\n                :normalImage=\"require('../../assets/ic_contacts_normal.png')\"\r\n                ></tabbaricon>通讯录\r\n            </mt-tab-item>\r\n            <mt-tab-item id=\"find\" @click.native=\"changeState(2)\">\r\n                <!-- <img style=\"width:25px;\" src=\"../../assets/ic_find_normal.png\" alt=\"\"> -->\r\n                <tabbaricon\r\n                :focused=\"currentIndex[2].isSelect\"\r\n                :selectImage=\"require('../../assets/ic_find_selected.png')\"\r\n                :normalImage=\"require('../../assets/ic_find_normal.png')\"\r\n                ></tabbaricon>发现\r\n            </mt-tab-item>\r\n            <mt-tab-item id=\"me\" @click.native=\"changeState(3)\">\r\n                <!-- <img style=\"width:25px;\" src=\"../../assets/ic_me_normal.png\" alt=\"\"> -->\r\n                <tabbaricon\r\n                :focused=\"currentIndex[3].isSelect\"\r\n                :selectImage=\"require('../../assets/ic_me_selected.png')\"\r\n                :normalImage=\"require('../../assets/ic_me_normal.png')\"\r\n                ></tabbaricon>我的\r\n            </mt-tab-item>\r\n        </mt-tabbar>\r\n        <!-- 按钮【图片/文字】 -->\r\n        <!-- 图片：子组件点击切换 -->\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n//1:引入顶部标题子组件\r\nimport TitleBar from \"./common/TitleBar.vue\"\r\n// 2.引入消息列表子组件\r\nimport MessageList from \"./common/MessageList.vue\"\r\n//3.引入底部导航条图片组件\r\nimport TabbarIcon from \"./common/TabbarIcon.vue\"\r\n\r\nexport default {\r\n    data(){\r\n        return{\r\n            active:\"message\",\r\n            currentIndex:[\r\n                {isSelect:true},//与第一个按钮图片相关\r\n                {isSelect:false},//与第二个按钮图片相关\r\n                {isSelect:false},//与第三个按钮图片相关\r\n                {isSelect:false},//与第四个按钮图片相关\r\n            ],\r\n        }\r\n    },\r\n //2:注册组件\r\n    components:{\r\n        \"titlebar\":TitleBar,//顶部 标题\r\n        \"messagelist\":MessageList,//消息列表\r\n        \"tabbaricon\":TabbarIcon,//tabbar组件\r\n    },\r\n    methods:{\r\n        //父组件定义函数,将其传递子组件\r\n        mysearch(){console.log(\"搜索\")},//顶部标题\r\n        myadd3(){console.log(\"添加\")},//消息列表\r\n        //为按钮绑定点击事件\r\n        //当前按钮图片，选中状态true其他是false\r\n        changeState(idx){\r\n            //功能：点底部按钮切换图片状态\r\n            //当前的图片是选中的，其他的是默认的？\r\n            //参数idx：表示下标\r\n            // console.log(idx);\r\n            // 1.创建循环遍历状态与当前元素下标相同\r\n            for (var i=0; i<this.currentIndex.length;i++){\r\n                // 2.判断如果参数与当前元素下标相同\r\n                if(i==idx){\r\n                    // 3.当前元素状态修改true\r\n                    this.currentIndex[i].isSelect=true;\r\n                }else{\r\n                    // 4.其他元素状态修改false\r\n                    this.currentIndex[i].isSelect=false;\r\n                }\r\n            }   \r\n        }\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n.page-tabbar{\r\n    overflow: hidden;/*溢出隐藏*/\r\n}\r\n.page-wrap{\r\n    overflow: auto;/*溢出显示滚动条*/\r\n    padding-bottom: 60px;/*底部导航条*/\r\n}\r\n/*覆盖原生组件中的样式*/\r\n/*覆盖默认文字颜色*/\r\n.mint-tabbar>.mint-tab-item{\r\n    color: #999;\r\n}\r\n/*覆盖选中文字颜色*/\r\n.mint-tabbar > .mint-tab-item.is-selected {\r\n    background-color: transparent;/*透明*/\r\n    color: #45c018;/*文字颜色*/\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":"src/components/message"}]}
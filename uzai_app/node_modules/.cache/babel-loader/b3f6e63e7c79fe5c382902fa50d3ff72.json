ebox(\"消息\", \"请先登录在购买\").then(function (res) {\n            _this.$router.push(\"/login\");\n          });\n        } else if (res.data.code == -2) {\n          //   -2添加失败\n          _this.$messagebox(\"消息\", \"添加失败\");\n        } else {\n          //   1添加成功\n          _this.$toast(\"添加成功\");\n        }\n\n        console.log(res);\n      });\n    },\n    loadMore: function loadMore() {\n      var _this2 = this;\n\n      // 功能一：当组件创建成功后获取第一页数据\n      //  1.创建url地址\n      var url = \"product\"; //1.1将当前页码加一\n\n      this.pno++;\n      var obj = {\n        pno: this.pno // 2.发送ajax请求获取第一页数据\n\n      };\n      this.axios.get(url, {\n        params: obj\n      }).then(function (res) {\n        // 3.将数据保存在data中\n        // console.log(res.data.data);\n        // this.list=res.data.data;\n        //数组拼接操作\n        var rows = _this2.list.concat(res.data.data); // 赋值\n\n\n        _this2.list = rows;\n      });\n    }\n  }\n};",{"version":3,"sources":["Product.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;AA2BA,eAAA;AACA,MADA,kBACA;AACA,WAAA;AACA,YAAA,EADA;AACA;AACA,WAAA,CAFA,CAEA;;AAFA,KAAA;AAIA,GANA;AAOA,SAPA,qBAOA;AACA,SAAA,QAAA;AACA,GATA;AAUA,WAAA;AACA,YADA,sBACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,KAHA;AAIA,WAJA,mBAIA,KAJA,EAIA;AAAA;;AACA;AACA;AACA,UAAA,MAAA,MAAA,MAAA,CAAA,OAAA,CAAA,GAAA;AACA,UAAA,QAAA,MAAA,MAAA,CAAA,OAAA,CAAA,KAAA;AACA,UAAA,QAAA,MAAA,MAAA,CAAA,OAAA,CAAA,KAAA;AACA,cAAA,GAAA,CAAA,KAAA,EANA,CAOA;;AACA,UAAA,MAAA,SAAA;AACA,UAAA,MAAA;AAAA,aAAA,GAAA;AAAA,eAAA,KAAA;AAAA,eAAA;AAAA,OAAA,CATA,CAUA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA;AAAA,gBAAA;AAAA,OAAA,EAAA,IAAA,CAAA,eAAA;AACA;AACA,YAAA,IAAA,IAAA,CAAA,IAAA,IAAA,CAAA,CAAA,EAAA;AACA;AACA,gBAAA,WAAA,CAAA,IAAA,EAAA,SAAA,EACA,IADA,CACA,eAAA;AACA,kBAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,WAHA;AAIA,SANA,MAMA,IAAA,IAAA,IAAA,CAAA,IAAA,IAAA,CAAA,CAAA,EAAA;AACA;AACA,gBAAA,WAAA,CAAA,IAAA,EAAA,MAAA;AACA,SAHA,MAGA;AACA;AACA,gBAAA,MAAA,CAAA,MAAA;AACA;;AACA,gBAAA,GAAA,CAAA,GAAA;AACA,OAhBA;AAiBA,KAhCA;AAiCA,YAjCA,sBAiCA;AAAA;;AACA;AACA;AACA,UAAA,MAAA,SAAA,CAHA,CAIA;;AACA,WAAA,GAAA;AACA,UAAA,MAAA;AAAA,aAAA,KAAA,GAAA,CACA;;AADA,OAAA;AAEA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA;AAAA,gBAAA;AAAA,OAAA,EAAA,IAAA,CAAA,eAAA;AACA;AACA;AACA;AACA;AACA,YAAA,OAAA,OAAA,IAAA,CAAA,MAAA,CAAA,IAAA,IAAA,CAAA,IAAA,CAAA,CALA,CAMA;;;AACA,eAAA,IAAA,GAAA,IAAA;AACA,OARA;AASA;AAlDA;AAVA,CAAA","sourcesContent":["<template>\r\n  <div class=\"product-app\">\r\n      <!-- <h1>Product.vue</h1> -->\r\n      <!-- 此div表示一个商品，循环 -->\r\n      <div class=\"goods-item\" v-for=\"(item,index) of list\" :key=\"index\">\r\n          <!-- 商品图片 -->\r\n          <!-- \"http://127.0.0.1:8080/02.jpg\" -->\r\n          <img :src=\"'http://127.0.0.1:8080/'+item.img_url\" />\r\n          <!-- 2.商品名称 -->\r\n          <h5>{{item.lname}}</h5>\r\n          <!-- 3.商品价格 -->\r\n          <div class=\"info\">{{item.price}}</div>\r\n          <!-- 4.加入购物车 -->\r\n          <mt-button @click=\"addcart\"\r\n            :data-lid=\"item.lid\"\r\n            :data-price=\"item.price\"\r\n            :data-lname=\"item.lname\"\r\n          >加入购物车</mt-button>\r\n      </div><!-- 商品结束 -->\r\n      <mt-button size=\"large\" @click=\"loadMore\">加载更多</mt-button>\r\n      <!-- 当前用户要点击加载更多，显示下一页数据 -->\r\n      <!-- 思考：保存上一页数据 -->\r\n      <mt-button @click=\"jumpCart\" size=\"large\">查看购物车</mt-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            list:[],//保存服务器返回商品列表\r\n            pno:0,//当前页码\r\n        }\r\n    },\r\n    created(){\r\n        this.loadMore();\r\n    },\r\n    methods:{\r\n        jumpCart(){\r\n            this.$router.push(\"/Cart\");\r\n        },\r\n        addcart(event){\r\n            // 功能：将商品添加至购物车\r\n            // 1.获取购物车中数据lid lanme price\r\n            var lid=event.target.dataset.lid;\r\n            var price=event.target.dataset.price;\r\n            var lname=event.target.dataset.lname;\r\n            console.log(lname);\r\n            // 2.创建url地址\r\n            var url = \"addcart\";\r\n            var obj = {lid:lid,lname:lname,price:price};\r\n            // 3.发送ajax请求获取\r\n            this.axios.get(url,{params:obj}).then(res=>{\r\n            // 4.获取服务器返回数据\r\n            if(res.data.code==-1){\r\n                // 5.-1提示先登录\r\n                this.$messagebox(\"消息\",\"请先登录在购买\")\r\n                .then(res=>{\r\n                    this.$router.push(\"/login\");\r\n                })\r\n            }else if(res.data.code==-2){\r\n                //   -2添加失败\r\n                this.$messagebox(\"消息\",\"添加失败\")\r\n            }else{\r\n                //   1添加成功\r\n                this.$toast(\"添加成功\")\r\n            }\r\n            console.log(res);\r\n            })\r\n        },\r\n        loadMore(){\r\n             // 功能一：当组件创建成功后获取第一页数据\r\n            //  1.创建url地址\r\n            var url = \"product\";\r\n            //1.1将当前页码加一\r\n            this.pno++;\r\n            var obj={pno:this.pno}\r\n            // 2.发送ajax请求获取第一页数据\r\n            this.axios.get(url,{params:obj}).then(res=>{\r\n                // 3.将数据保存在data中\r\n                // console.log(res.data.data);\r\n                // this.list=res.data.data;\r\n                //数组拼接操作\r\n                var rows=this.list.concat(res.data.data);\r\n                // 赋值\r\n                this.list=rows;\r\n            })\r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n/* 1.最外层的父元素，弹性布局 */\r\n.product-app{\r\n    display:flex;/*弹性布局*/\r\n    flex-wrap: wrap;/*指定子元素换行*/\r\n    justify-content: space-between;/*两端对齐*/\r\n    padding: 4px;/*内边距*/\r\n    text-align: center;\r\n}\r\n/* 2.当前商品 49% */\r\n.goods-item{\r\n    width:49%; /*宽*/\r\n    border:1px solid #ccc; /*灰色边框*/\r\n    border-radius:5px; /*圆角*/\r\n    margin:2px 0;\r\n    padding:2px;\r\n    box-sizing:border-box; /*重新计算元素宽度*/\r\n    display: flex;\r\n    flex-direction:column; /*商品内容按列排放*/\r\n    min-height: 249px; /*高*/\r\n}\r\n/* 3.修饰当前商品中的图片 */\r\n.goods-item img{\r\n    width:100%; \r\n}\r\n/* 4.修饰当前商品的价格 */\r\n.info{\r\n    color:red;\r\n    font-size:19px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/xz"}]}